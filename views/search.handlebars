{{>navbar}}

<div class="container">
  <div class="row">
    <div class="col s12 left">
      <h3 id="search">Search</h3>

      <table id="myTable" class="display" cellspacing="0" style="width: 100%;">

        <thead>
          <tr>
            <th>Store</th>
            <th>City</th>
            <th>State</th>
            <th>ZipCode</th>
            {{!-- <th>Delete</th> --}}
          </tr>
        </thead>

        <tbody>
          {{#each items}}
          <tr>
            <td>{{dataValues.store}}</td>
            <td>{{dataValues.city}}</td>
            <td>{{dataValues.state}}</td>
            <td>{{dataValues.zipcode}}</td>
            {{!-- <td id="delete-item"><a href class="editor_remove">Delete</a></td> --}}
          </tr>
          {{/each}}
        </tbody>

      </table>

      <div><a id="submit-btn" type="submit" class="btn col" href="/input">Go Back</a></div>

    </div>
  </div>
</div>

{{>footer}}

<script src="/js/search.js" type="text/javascript"></script>
